SELECT dev_id,SUM(payload_fields.waterusage) OVER (PARTITION BY dev_id LIMIT DURATION (minute, 5))  AS waterusage
INTO
    [alert]
FROM
    [SMWSB-IOTHUB] AS IOT
group by dev_id, TumblingWindow(minute,30)


SELECT dev_id,SUM(payload_fields.waterusage)
INTO [dayalert]
FROM [SMWSB-IOTHUB] AS IOT
WHERE SUM(payload_fields.waterusage) OVER (PARTITION BY dev_id LIMIT DURATION (hour, 1)) > 100
group by dev_id, TumblingWindow(minute,60)
